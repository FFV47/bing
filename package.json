{
  "name": "bing-auto-search",
  "version": "1.0.0",
  "description": "Cross-platform application that automates Bing searches at random intervals",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "bing-auto-search": "./dist/cli.js"
  },
  "scripts": {
    "start": "node dist/cli.js",
    "dev": "node --watch src/cli.js",
    "dev:src": "node src/cli.js",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "bing",
    "search",
    "automation",
    "cli",
    "cross-platform"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "puppeteer": "^24.33.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^6.0.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "es5",
    "quoteProps": "preserve"
  }
}
