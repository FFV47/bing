{
  "name": "bing-auto-search",
  "version": "1.0.0",
  "description": "Application that opens browser and searches on Bing at determined intervals",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "bing",
    "search",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.34.0",
    "puppeteer": "^24.32.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@eslint/json": "^0.14.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
